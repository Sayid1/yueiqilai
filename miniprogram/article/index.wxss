.wrap {
  font-size: 34rpx;
  color: #222;
}
.content {
  min-height: 300rpx;
}
.other view {
  margin: 0 -30rpx;
  height: 90rpx;
  line-height: 90rpx;
  padding-left: 30rpx;
  border-bottom: 1px solid #f2f2f2;
}
.fixed-bottom {
  display: flex;
  justify-content: space-between;
}
.fixed-bottom button {
  color: #232122;
  font-weight: 600;
  font-size: 36rpx;
  background: #FFE554;
  width: 70%;
}
.fixed-bottom button.full {
  flex: 1;
}
.fixed-bottom button.small {
  background: yellowgreen;
  width: 30%;
}